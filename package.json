    "@sentry/browser": "^5.30.0",
    "@yellowspot/ng-truncate": "^1.7.0",
    "bpmn-js": "^6.5.1",
    "bpmn-js-properties-panel": "^0.33.2",
    "camunda-bpmn-moddle": "^4.4.0",
    "camunda-dmn-moddle": "^1.0.0",
    "diagram-js": "^6.6.1",
